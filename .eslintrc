{
    "parser": "babel-eslint",
    "plugins": ["react", "react-native"],
    "extends": "airbnb",
    "rules": {
        "quotes": ["error", "single"],
        "no-use-before-define": 0,
        "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
        "strict": 0,
        "no-console": 0,
        "class-methods-use-this": 0,
        "func-names": 0,
        "no-unused-vars": 0,
        "react/require-default-props": 0,
        "no-nested-ternary": 0,
        "camelcase": ["error", { "ignoreDestructuring": true }]
    }
}
