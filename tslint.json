{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-curly-spacing": ["always"],
    "ordered-imports": false,
    "max-line-length": [true, 140],
    "interface-name": [true, "never-prefix"],
    "variable-name": false,
    "quotemark": [true, "single"],
    "indent": [true, "spaces"]
  }
}
