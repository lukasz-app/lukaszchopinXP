{
    "name": "lukaszchopin",
    "private": true,
    "main": "index.js",
    "version": "0.0.1",
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly",
        "build:web": "cross-env platform=web webpack --config=web/webpack/prod.js --progress --colors --mode=production",
        "connected-iphone": "instruments -s devices | grep '.* iPhone' -o",
        "lint": "tslint -p tsconfig.json -r tslint.json",
        "rn-cli": "node node_modules/react-native/local-cli/cli.js",
        "start:android": "yarn rn-cli run-android",
        "start:android:port": "yarn rn-cli run-android --port 8082",
        "start:ios:device": "yarn rn-cli run-ios --device=\"$(npm run --silent connected-iphone)\"",
        "start:ios": "yarn rn-cli run-ios",
        "start:rn-dev-server": "yarn rn-cli start --reset-cache",
        "start:web": "cross-env platform=web webpack-dev-server --config=web/webpack/dev.js --progress --colors --mode=development",
        "start:windows": "yarn rn-cli run-windows",
        "test:debug": "node --inspect-brk node_modules/.bin/jest -c jest/jest.config.js --runInBand",
        "test:watch": "yarn test --watch",
        "test": "jest -c jest/jest.config.js",
        "type-check:watch": "yarn type-check -w",
        "type-check": "tsc --noEmit",
        "dev": "code . && yarn start:web && yarn start:android:port",
        "postinstall": "rndebugger-open"
    },
    "dependencies": {
        "mobx": "4.3.1",
        "mobx-persist": "0.4.1",
        "mobx-react": "5.4.2",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "react-native": "0.57.8",
        "react-native-devmenu-trigger": "^1.1.0",
        "react-native-iphone-x-helper": "^1.2.0",
        "react-native-windows": "0.57.0-rc.0",
        "reactxp": "1.5.0",
        "reactxp-virtuallistview": "^1.5.0"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/plugin-proposal-decorators": "7.2.3",
        "@babel/preset-env": "7.2.3",
        "@types/enzyme": "3.1.15",
        "@types/jest": "23.3.10",
        "@types/react": "16.4.18",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.4",
        "compression-webpack-plugin": "2.0.0",
        "cross-env": "5.2.0",
        "enzyme": "3.8.0",
        "enzyme-adapter-react-16": "1.7.1",
        "enzyme-to-json": "3.3.5",
        "eslint": "5.9.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-react": "7.11.1",
        "eslint-plugin-react-native": "3.5.0",
        "fork-ts-checker-webpack-plugin": "0.5.2",
        "html-webpack-plugin": "3.2.0",
        "jest": "23.6.0",
        "metro-react-native-babel-preset": "0.51.1",
        "react-native-debugger-open": "0.3.17",
        "rnpm-plugin-windows": "0.2.8",
        "tslint": "5.12.0",
        "tslint-react": "3.6.0",
        "typescript": "^3.2.2",
        "webpack": "4.28.2",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.14",
        "webpack-merge": "4.1.5"
    }
}
